{
  "name": "hnpwa-with-react",
  "private": true,
  "version": "v1.0.0",
  "description": "HNPWA with React",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js --env.dev --inline --watch",
    "start:server": "cross-env NODE_ENV=development node server",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --env.prod --profile --json > stats.json && yarn analyze:cache",
    "analyze:cache": "webpack-bundle-analyzer stats.json bundleDir=build",
    "build": "yarn lint && cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --env.prod",
    "build:server": "cross-env NODE_ENV=production node scripts/build.server.js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint app test --ext .js,.jsx",
    "lint:scss": "sh ./scripts/scss-lint.sh app/",
    "ncu": "ncu",
    "precommit": "lint-staged",
    "prepush": "yarn lint",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "coverage": "open ./coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.{css,scss}": "sh ./scripts/scss-lint.sh"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "koa": "^2.3.0",
    "koa-static": "^4.0.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-server": "^4.2.2",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-info": "^3.2.10",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/addon-options": "^3.2.10",
    "@storybook/react": "^3.2.10",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "errorhandler": "^1.5.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "ignore-loader": "^0.1.2",
    "jest": "^21.0.2",
    "lint-staged": "^4.2.2",
    "nock": "^9.0.21",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.12.1",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "preload-webpack-plugin": "^2.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^4.0.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
